{
  "compilerOptions": {
    "lib": [
      "es2019"
    ],
    "module": "commonjs",
    "types": ["jest", "node", "@types/jest"],
    "target": "es2019",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": [
    "src",
    "test",
    "testSetup.ts"  
  ],
  "exclude": [
    "cypress",
    "test/stress",
  ]
}